{
    "name": "@elusiv/sdk",
    "version": "0.1.13",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "lint": "eslint --ext .ts src/ test/",
        "lint:fix": "eslint --ext .ts src/ test/ --fix",
        "clean": "rm -rf dist/ && rm -rf build/",
        "compile": "npm run clean && tsc",
        "bundle": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup --config config/rollup.config.js && ./scripts/buildRelease.sh",
        "build": "npm run clean && npm run compile && npm run bundle",
        "docs": "set -ex; typedoc --treatWarningsAsErrors --excludeProtected",
        "test": "mocha 'test/**/*.{ts,js}' --exit",
        "test-single": "mocha $1 --exit",
        "main": "npm run clean && tsc && node dist/main.js",
        "update-token": "./scripts/updateToken.sh"
    },
    "dependencies": {
        "@dao-xyz/borsh": "^5.1.5",
        "@noble/hashes": "^1.1.5",
        "@pythnetwork/client": "^2.17.0",
        "@solana/spl-token": "^0.3.5",
        "@solana/web3.js": "^1.75.0",
        "aes-js": "^3.1.2",
        "bs58": "^5.0.0",
        "elusiv-circuits": "git@github.com:elusiv-privacy/circuits.git#nico-metadata-optional-fee",
        "elusiv-cryptojs": "git@github.com:elusiv-privacy/elusiv-cryptojs.git",
        "elusiv-serialization": "git@github.com:elusiv-privacy/serialization.js.git",
        "node-fetch": "^3.3.1"
    },
    "devDependencies": {
        "@iarna/toml": "^2.2.5",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@rollup/plugin-virtual": "^3.0.1",
        "@types/aes-js": "^3.1.1",
        "@types/bn.js": "^5.1.1",
        "@types/bs58": "^4.0.1",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.5",
        "@types/crypto-js": "^4.1.1",
        "@types/mocha": "^10.0.1",
        "@types/node-fetch": "^2.6.3",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.0",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.40.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.4",
        "mocha": "^10.2.0",
        "rollup": "^3.20.0",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-polyfill-node": "^0.11.0",
        "ts-node": "^10.4.0",
        "typedoc": "^0.24.7",
        "typescript": "^4.9.4"
    },
    "author": "Elusiv Privacy",
    "description": "Client SDK for interacting with Elusiv on-chain programs",
    "homepage": "https://elusiv.io",
    "bugs": {
        "url": "https://github.com/elusiv-privacy/elusiv-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/elusiv-privacy/elusiv-sdk.git"
    },
    "keywords": [
        "Zero Knowledge",
        "Cryptography",
        "Privacy",
        "Blockchain",
        "Solana",
        "Elusiv",
        "Library"
    ],
    "license": "ISC"
}